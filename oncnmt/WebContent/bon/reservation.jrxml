	<queryString><![CDATA[SELECT distinct t.trans_type||'-'||t.whs_id||'-'||t.trans_no,v.res_app_whs,a.art_art_libelle||'/'||nvl(v.profil_id,'_'),v.quantite_article,
nvl(v.res_app_hlr,'_'),nvl(v.res_app_pq,'_'),t.trans_date,gss_actor.act_name,d.dest_nom,
t.trans_ext_ref as commande,t.trans_notes,gss_etat_concordance.etat_title,gss_etat_concordance.etat_from_label,
gss_etat_concordance.etat_to_label,
  CASE
              WHEN t.approver in ('N')
              THEN 'non' 
              
            WHEN t.approver in ('P')
              THEN 'prouver' 
              
          END  prouver,
          
            (select app.approv_date 
  from gss_approbation app  
where   app.trans_no=$P!{param3} and   
app.whs_id=$P!{param2} AND
app.trans_type=$P!{param1}  ) as date_valid ,

  (select a.act_name  
  from gss_approbation app ,gss_actor a 
          where  app.trans_no=$P!{param3} and   
app.whs_id=$P!{param2} AND
app.trans_type=$P!{param1} and 
a.act_id=app.approv_act) as acteur_valid,


  (select app.no_cheque  
  from gss_approbation app 
          where   app.trans_no=$P!{param3} and   
app.whs_id=$P!{param2} AND
app.trans_type=$P!{param1}  )as cheque  ,

  (select app.note 
  from gss_approbation app 
          where   app.trans_no=$P!{param3} and   
app.whs_id=$P!{param2} AND
app.trans_type=$P!{param1}  )as note_approbation 


from gss_reserv_approb v ,gss_destinataire d,gss_transaction t,gss_article a,gss_warehouse w,gss_actor,
gss_etat_concordance 


where v.trans_type=t.trans_type and v.whs_id=t.whs_id and v.trans_no=t.trans_no and 
(a.art_id=v.art_id) and gss_actor.act_id=t.act_id and t.dest_id=d.dest_id and 
(gss_etat_concordance.trans_type(+)=t.trans_type)and 
(t.trans_no=$P!{param3}) and
(t.whs_id=$P!{param2})and
(t.trans_type=$P!{param1})]]></queryString>

