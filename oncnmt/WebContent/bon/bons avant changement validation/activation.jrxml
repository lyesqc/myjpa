	<queryString><![CDATA[select distinct gss_transaction.trans_type,gss_transaction.whs_id,gss_transaction.trans_no,
gss_transaction.trans_date,gss_transaction.trans_notes,gss_actor2.act_name,gss_range.art_id,gss_article.art_art_libelle,gss_article_profil.profil_libelle,
gss_range.rng_start,gss_range.rng_end,gss_range.rng_end-gss_range.rng_start+1 as qte,
gss_etat_concordance.etat_title,gss_etat_concordance.etat_from_label,
gss_etat_concordance.etat_to_label,
case when gss_article.art_type_id in ('S1','S2') then decode(substr(gss_range.rng_start, 4, 1),'0', substr(gss_range.rng_start, 4, 2),'1', substr(gss_range.rng_start, 4, 2),'2', substr(gss_range.rng_start, 4, 2), '0' || substr(gss_range.rng_start, 5, 1)) else '_' end hlr,
d.dest_nom as  DESTINATAIRE,

  CASE
              WHEN gss_transaction.trans_type in ('DPO','CAP','CPP')
              THEN gss_article_profil.profil_libelle
              
            WHEN gss_transaction.trans_type in ('DAP','DPP')
              THEN substr(gss_range.rng_promotion, 9)
              
              WHEN gss_transaction.trans_type = 'AFS'
              THEN gss_range.rng_promotion
              
              
     
       END  promotion, 

         CASE
              WHEN gss_transaction.annuler in ('N')
              THEN 'non' 
              
            WHEN gss_transaction.annuler in ('A')
              THEN 'annuler' 
              
          END  annuler,
   
  (select app.approv_date 
  from gss_approbation app  
where   app.trans_no=$P!{param3} and   
app.whs_id=$P!{param2} AND
app.trans_type=$P!{param1}  ) as date_annuler ,


  (select app.note  
  from gss_approbation app 
          where   app.trans_no=$P!{param3} and   
app.whs_id=$P!{param2} AND
app.trans_type=$P!{param1}  )as note_annuler  ,

  (select a.act_name  
  from gss_approbation app ,gss_actor a 
          where  app.trans_no=$P!{param3} and   
app.whs_id=$P!{param2} AND
app.trans_type=$P!{param1} and 
a.act_id=app.approv_act) as acteur_annuler


from gss_transaction,gss_range,gss_actor2,gss_article_profil,
gss_article,gss_etat_concordance,gss_destinataire d 


where 
(gss_transaction.dest_id=d.dest_id(+)) and 
(gss_transaction.act_id=gss_actor2.act_id) and
(gss_range.art_id=gss_article.art_id) and
(gss_article_profil.profil_id(+)=gss_range.profil_id)and
(gss_range.trans_no=gss_transaction.trans_no)and
(gss_range.whs_id=gss_transaction.whs_id)and
(gss_range.trans_type=gss_transaction.trans_type) and
(gss_etat_concordance.trans_type(+)=gss_transaction.trans_type)and 
(gss_transaction.trans_no=$P!{param3}) and
(gss_transaction.whs_id= $P!{param2})and
(gss_transaction.trans_type= $P!{param1})]]></queryString>

