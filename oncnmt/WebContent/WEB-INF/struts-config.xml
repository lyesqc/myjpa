      <forward name="parametreartcle" path="/forms/parametre_article.jsp"/>
      <forward name="parametrwarehouse" path="/forms/parametre_warehouse.jsp"/>
    </action>
    
      <action path="/afficherparametre" name="parametre1Form" type="com.yourcompany.struts.action.AfficherparametreAction" attribute="parametre1Form">
      <forward name="parametre" path="/forms/parametre.jsp"/>
    </action>
