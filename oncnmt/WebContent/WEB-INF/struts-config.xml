      <forward name="listprofil" path="/forms/listprofil.jsp"/>
      <forward name="menu" path="/forms/menuPrincipal.jsp"/>
    </action>
    <action path="/rapport_global" name="etaStkForm" type="com.yourcompany.struts.action.Rapport_globalAction" scope="session" parameter="hidden" attribute="etaStkForm">
      <forward name="rapportglobal" path="/forms/popup_rapport_global.jsp"/>
      <forward name="rapportreservation" path="/forms/rapportreservationglobal.jsp"/>
    </action>
