      <forward name="naima" path="/forms/naima2.jsp"/>
    </action>
    <action  name="recepxlsForm" path="/recepxls" scope="session" parameter="hidden" type="com.yourcompany.struts.action.RecepxlsAction" attribute="recepxlsForm">
  
