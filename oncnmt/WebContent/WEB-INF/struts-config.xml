    </action>
    <action path="/treeview" type="fr.improve.struts.taglib.layout.treeview.TreeviewAction" scope="request" validate="false">
    </action>
    <action path="/main" name="loginForm" type="com.yourcompany.struts.action.MainAction" scope="request" attribute="loginForm">
      <forward name="initial" path="/forms/login.jsp"/>
    </action>
    <action path="/login" name="loginForm" type="com.yourcompany.struts.action.LoginAction" scope="request" attribute="loginForm">
      <forward name="login" path="/forms/login.jsp"/>
      <forward name="menu" path="/forms/menuPrincipal.jsp"/>
      <forward name="listprofil" path="/forms/listprofil.jsp"/>
    </action>
    <action path="/preparRecep" name="recepForm" type="com.yourcompany.struts.action.PreparRecepAction" scope="session" attribute="recepForm">
      <forward name="preparer1" path="/forms/recep.jsp"/>
    </action>
    <action path="/recherche" name="rechercheForm" type="com.yourcompany.struts.action.RechercheAction" scope="session" attribute="rechercheForm">
      <forward name="recherche" path="/forms/recherche.jsp"/>
    </action>
    <action path="/recherche1" name="rechercheForm" type="com.yourcompany.struts.action.Recherche1Action" scope="session" parameter="hidden" attribute="rechercheForm">
      <forward name="visualiser" path="/forms/recherche.jsp"/>
    </action>
    <action path="/preparStk" name="etaStkForm" type="com.yourcompany.struts.action.PreparStkAction" scope="session" attribute="etaStkForm">
      <forward name="prepar" path="/forms/stock.jsp"/>
    </action>
    <action path="/stock" name="etaStkForm" type="com.yourcompany.struts.action.StockAction" scope="session" parameter="hidden" attribute="etaStkForm">
      <forward name="EtatDeStock" path="/forms/stock.jsp"/>
      <forward name="etat" path="/forms/popupetatdist.jsp"/>
      <forward name="pophistorique" path="/forms/pophistorique.jsp"/>
    </action>
    
   <action path="/envoi" name="envoiForm" type="com.yourcompany.struts.action.EnvoiAction" scope="session" parameter="hidden" attribute="envoiForm">
      <forward name="envoipac" path="/forms/envoiePackaging.jsp"/>
