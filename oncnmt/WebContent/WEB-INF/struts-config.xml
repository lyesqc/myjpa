    <action path="/return" name="returnForm" type="com.yourcompany.struts.action.Return" parameter="hidden" attribute="returnForm">
      <forward name="auto" path="/forms/return.jsp"/>
      <forward name="listaprob" path="/forms/listaprob.jsp"/>
      <forward name="affichaprob" path="/forms/affichaprob.jsp"/>
      <forward name="autoreserv" path="/forms/autosorti_reserv.jsp"/>
      <forward name="menu" path="/forms/menuvalider.jsp"/>
    </action>
    
