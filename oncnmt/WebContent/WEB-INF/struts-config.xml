      <forward name="menu" path="/forms/menuvalider.jsp"/>
    </action>
    <action path="/afficher3" name="envoiForm" type="com.yourcompany.struts.action.Afficher3Action" attribute="envoiForm">
      <forward name="combo3" path="/forms/combo3.jsp"/>
    </action>
    <action path="/afficher4" name="envoipackagForm" type="com.yourcompany.struts.action.Afficher4Action" attribute="envoipackagForm">
      <forward name="combo4" path="/forms/combo4.jsp"/>
    </action>
    <action path="/afficher" name="transfert1Form" type="com.yourcompany.struts.action.AfficherAction" attribute="transfert1Form">
      <forward name="combo" path="/forms/combo.jsp"/>
    </action>
