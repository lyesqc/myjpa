      <forward name="envoitrans" path="/forms/envoitrans.jsp"/>
      <forward name="listordretrans" path="/forms/listordretrans.jsp"/>
      <forward name="menu" path="/forms/menuvalider.jsp"/>
    </action>
    <action path="/receptrans" name="transfert1Form" type="com.yourcompany.struts.action.ReceptransAction" scope="session" parameter="hidden" attribute="transfert1Form">
      <forward name="listenvoitrans" path="/forms/listenvoitrans.jsp"/>
