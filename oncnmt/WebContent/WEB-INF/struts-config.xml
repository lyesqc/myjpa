      <forward name="rech2" path="/forms/edit.jsp"/>
    </action>
    
    <action path="/preparmodif" name="parametreForm" type="com.yourcompany.struts.action.PreparmodifAction" scope="request" attribute="parametreForm">
      <forward name="modif" path="/forms/modifacteur.jsp"/>
